# OpenRouter API Key
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your-openrouter-api-key-here

# E2B Sandbox API Key (optional, for code execution)
# Get your API key from: https://e2b.dev/dashboard?tab=keys
E2B_API_KEY=e2b_***

# Default Model (optional, can be set via init command)
DEFAULT_MODEL=anthropic/claude-3.5-sonnet

# Budget Limit (optional, in USD)
BUDGET_LIMIT=10.00

# E2B Sandbox Configuration (optional)
E2B_TIMEOUT_MS=300000
E2B_AUTO_CLEANUP=true
